{% extends "layout-sidebar.twig" %}

{% set page_header = {
  title: 'Latest Thinking',
  breadcrumb: [
    { text: 'Leadership and Impact' }
  ]
} %}

{% block layout_content %}

  {% include 'partials/story-teaser.twig' with {
    id: 'featured',
    featured: true
  } %}

  <ol class="list list--dividers list--lg mb-6">
    {% for i in 0..5 %}
      <li class="list__item">
        {% include 'partials/story-teaser.twig' with { id: i } %}
      </li>
    {% endfor %}
  </ol>

  {% include 'partials/pagination.twig' %}

{% endblock %}
