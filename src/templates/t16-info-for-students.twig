{% extends "layout.twig" %}

{% set page_header = {
  title: 'Information for New Students',
  breadcrumb: [
    { text: 'Student Services', href: '#' }
  ]
} %}

{% block page %}

  <div class="mb-6 text-right">
    <figure>
      {% include 'partials/img.twig' with { name: 'top_level' } %}
      <figcaption class="media__caption">some caption for the image</figcaption>
    </figure>
  </div>

  <div class="container">
    <div class="row">
      <div class="col-md-push-1 col-md-9">

        {% include 'partials/callout-contacts.twig' %}

        {% include 'paragraphs/related-stories.twig' %}

        {% for section in quicklinks %}
          {% include 'partials/callout-links.twig' with section %}
        {% endfor %}

        {% include 'paragraphs/related-events.twig' %}

        {% include 'paragraphs/related-basic-content.twig' %}

      </div>
    </div>
  </div>

  {#TODO: PSD has pagination/load more button but do we want that?#}

{% endblock %}
