{
  "name": "frontend",
  "description": "Frontend files and build tools for new miis.edu and other projects to come",
  "version": "0.0.1",
  "main": "gulpfile.babel.js",
  "scripts": {
    "start": "gulp",
    "dev": "gulp dev",
    "build": "NODE_ENV=production gulp build",
    "build:pardot": "npm run build -- --imagesDir=https://www.middlebury.edu/institute/themes/custom/institute_theme/images/ && gulp replaceImagePaths --imagesDir=https://www.middlebury.edu/institute/themes/custom/institute_theme/images/",
    "build:icons": "gulp buildIcons",
    "now-build": "npm run build && mv dist public",
    "deploy": "gulp deploy",
    "format": "prettier --config ./.prettierrc --write \"src/**/*.{js,scss}\"",
    "lint": "eslint src/js/**/*.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/middlebury/frontend.git"
  },
  "bugs": {
    "url": "https://github.com/middlebury/frontend/issues"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-transform-react-jsx": "^7.7.0",
    "@babel/preset-env": "^7.4.5",
    "@rollup/plugin-commonjs": "^11.0.1",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "autoprefixer": "^9.6.0",
    "beeper": "^2.0.0",
    "browser-sync": "^2.26.7",
    "css-mqpacker": "^7.0.0",
    "cssnano": "^4.1.10",
    "del": "^4.1.1",
    "dotenv": "^8.0.0",
    "eslint": "^6.0.1",
    "eslint-config-drupal": "^4.0.1",
    "gulp": "^4.0.2",
    "gulp-data": "^1.3.1",
    "gulp-if": "^3.0.0",
    "gulp-imagemin": "^5.0.3",
    "gulp-notify": "^3.2.0",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^8.0.0",
    "gulp-prettify": "^0.5.0",
    "gulp-rename": "^1.4.0",
    "gulp-replace": "^1.0.0",
    "gulp-sass": "^4.0.2",
    "gulp-size": "^3.0.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-svg-sprite": "^1.5.0",
    "gulp-twig": "^1.2.0",
    "js-yaml": "^3.13.1",
    "postcss-url": "^8.0.0",
    "prettier": "^1.18.2",
    "rollup": "^1.29.1",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-filesize": "^6.2.1",
    "rollup-plugin-sizes": "^1.0.1",
    "rollup-plugin-uglify": "^6.0.4",
    "sort-css-media-queries": "^1.5.0",
    "yargs": "^13.2.4"
  },
  "dependencies": {
    "chart.js": "^2.9.2",
    "countup.js": "^2.0.4",
    "intersection-observer": "^0.5.0",
    "mdn-polyfills": "^5.18.0",
    "micromodal": "git+https://git@github.com/zebapy/micromodal.git",
    "nodelist-foreach-polyfill": "^1.2.0",
    "normalize.css": "^5.0.0",
    "object-fit-images": "^3.2.3",
    "pikaday": "^1.7.0",
    "preact": "^10.0.4",
    "swiper": "^4.1.0"
  },
  "resolutions": {
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.11",
    "minimatch": "^3.0.2"
